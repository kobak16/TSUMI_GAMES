<div class="container">
  <div class="row">
    <div class="col-md-4">
      <h2>フォロー一覧</h2>
      <% @users.each do |user| %>
        <div class="col-md-4 bg-white border dorder-dark rounded m-1 d-block">
          <%= link_to user_path(user.id), class: 'text-dark' do %>
            <h3><%= user.username %></h3>
          <% end %>
        </div>
      <% end %>
    </div>
  </div>
</div>